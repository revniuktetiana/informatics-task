<html lang="en">
  <head>
    <title>JavaScript</title>
  </head>
  <body>
    <script>
    let user = {}
    user.name = "John"
    user.surname = "Smith"
    user.name = "Pete"
    delete user.name
    
    console.log(user)
    
    const user1 = { name: "John" }
    const admin = user1
    
    admin.name = "Pete"
    
    console.log(user1)
    console.log(admin)
    
    const user2 = {
        name: "John",
        age: 30
    }
    
    const userCopy = Object.assign({}, user2)
    
    user2.name = "Ann"
    
    console.log(user2)
    console.log(userCopy)
    
    let salaries = {
        John: 100,
        Ann: 160,
        Pete: 130,
    }
    const sum = salaries
    
    console.log(sum)
    </script>
  </body>
